	Journal = {Science},
	Number = {5796},
	Pages = {2005--2007},
	Pmid = {17023657},
	Publisher = {American Association for the Advancement of Science},
	Title = {{Large Punctuational Contribution of Speciation to Evolutionary Divergence at the Molecular Level}},
