	Keywords = {acanthoparypha,arctic,canada,holia glabra,hyrokybe lightfooti,hyrokybe mitchellae,hyrokybe youngi,northwest territories,pandaspinapyga,parayoungia mclaughlini,silurian,taxonomy,trilobita,youngia},
	Number = {4},
	Pages = {698--718},
	Publisher = {Paleontological Society},
	Title = {Systematics of the {Acanthoparyphinae (Trilobita)}, with species from the {Silurian of Arctic Canada}},
